pipeline {
    agent any{
    stage('Newman Tests API') {
        steps {
            sh '''
            cd API/newman
            newman run Examen.postman_collection.json -e Examen.postman_environment.json
            '''
        }
    }//mirka

    stage('JMeter Tests') {
        steps {
            sh '''
            ls
            export PATH=/home/pjcalvov/jmeter/apache-jmeter-5.3/bin:$PATH
            cd LOAD
            jmeter -n -t TestPlanExamen.jmx
            '''
        }
    }
    }
}


    